<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<display-name>PinAgent</display-name>
	<description>PinAgent</description>
	<servlet>
		<servlet-name>Processor</servlet-name>
		<servlet-class> com.safenetinc.viewpin.agent.Processor 
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Processor</servlet-name>
		<url-pattern>/Processor/RequestPin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Processor</servlet-name>
		<url-pattern>/Processor/ReceivePin.js</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>2</session-timeout>
	</session-config>
</web-app>